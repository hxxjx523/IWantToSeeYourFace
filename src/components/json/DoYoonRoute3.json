[
    {"name": "김여주", "text": "응, 그런데?", "img": "./images/Kimyeoju/Kimyeoju_face5.png", "window": "./images/dialogueWindow/Kimyeoju_dialogueWindow1.png", "background": ""},
    {"name": "도윤", "text": "나.. 도윤인데 기억나?", "img": "./images/Doyoon/Doyoon_face2-1.png", "window": "./images/dialogueWindow/Doyoon_dialogueWindow.png", "background": ""},
    {"name": "김여주", "text": "도윤.. 도윤.. 어디서 많이 들어본 이름인데…", "img": "./images/Kimyeoju/Kimyeoju_face11.png", "window": "./images/dialogueWindow/Kimyeoju_dialogueWindow1.png", "background": ""},
